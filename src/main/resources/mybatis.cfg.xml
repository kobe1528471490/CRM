<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="logImpl" value="SLF4J"/>
        <!-- 开启二级缓存 -->
        <setting name="cacheEnabled" value="true"/>
    </settings>

    <!-- 别名 -->
    <typeAliases>
        <typeAlias type="com.zking.crm.role.model.Sysuser" alias="Sysuser"/>
        <typeAlias type="com.zking.crm.role.model.Sysright" alias="Sysright"/>
        <typeAlias type="com.zking.crm.role.model.Sysrole" alias="Sysrole"/>
        <typeAlias type="com.zking.crm.role.model.Sysroleright" alias="Sysroleright"/>




    </typeAliases>

    <plugins>
        <plugin interceptor="com.github.pagehelper.PageInterceptor">
        </plugin>
    </plugins>
</configuration>